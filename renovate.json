{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": true,
  "extends": [
    "config:recommended"
  ],
  "assignees": [
    "grzenkom"
  ],
  "ignorePaths": [
    "export/"
  ],
  "labels": [
    "Type::Maintenance"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "prHourlyLimit": 10,
  "rebaseWhen": "behind-base-branch",
  "separateMajorMinor": true,
  "separateMinorPatch": true,
  "separateMultipleMajor": true
}